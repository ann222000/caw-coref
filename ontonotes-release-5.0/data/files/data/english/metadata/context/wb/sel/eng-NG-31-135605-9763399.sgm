FUNCTION __StrList2Array        (cList AS STRING)       AS ARRAY PASCAL
LOCAL nPos      AS DWORD
LOCAL cTemp     AS STRING
LOCAL c         AS STRING
LOCAL aRet      AS ARRAY

aRet  := {}
cTemp := cList

IF SLen(cTemp) == 0
RETURN aRet
ENDIF

nPos  := At2(&quot;,&quot;, cTemp)
DO WHILE nPos &gt; 0
c     := SubStr3(cTemp, 1, nPos-1)
cTemp := SubStr2(cTemp, nPos+1)
//      UH 06/21/1999
//      AAdd(aRet, StrTran(c, &quot; &quot;, &quot;&quot;) )
AAdd(aRet, AllTrim(c))
nPos := At2(&quot;,&quot;, cTemp)
ENDDO

IF SLen(cTemp) &gt; 0
AAdd(aRet, AllTrim(cTemp) )
ENDIF

RETURN aRet

